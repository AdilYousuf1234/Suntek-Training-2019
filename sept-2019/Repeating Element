#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n],b[n],i;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        b[i]=-1;
    }
    for(i=0;i<n;i++)
    {
        if(b[a[i]]==-1)
        {
            b[a[i]]=1;
        }
        else
        {
        printf("\n%d",a[i]);
        break;
        }
            
        }
    
}
